#include<iostream>
#include<string>
using namespace std;

int main() {
    string str;
    cout<<"Enter string: ";
    cin>>str;
    int i;
    for(i=str.length()-1;i>=0;i--) {
        cout<<str[i];
    }
    return 0;
}
/*/ OUTPUT:
Enter string: HELLO
OLLEH
/*/
